{
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron dist/index.js",
    "build": "webpack",
    "dev": "concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\"",
    "react:dev": "webpack serve --mode=development",
    "electron:dev": "nodemon",
    "electron:prod": "electron-builder build --win --x64 --config=./configuration/electron-builder.yml"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^1.2.0",
    "html-webpack-plugin": "^5.1.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
